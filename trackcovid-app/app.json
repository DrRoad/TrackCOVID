{
  "expo": {
    "name": "TrackCOVID",
    "slug": "trackcovid",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.tyleryasaka.trackcovid",
      "buildNumber": "0.0.0",
      "supportsTablet": true
    },
    "description": "Peer-to-peer, privacy-preserving contact tracing. Feel free to try out this Android prototype, but the best way to use TrackCOVID at the moment is through the web app. Learn more at https://trackcovid.net.",
    "githubUrl": "https://github.com/tyleryasaka/TrackCOVID/tree/master/trackcovid-app"
  }
}
